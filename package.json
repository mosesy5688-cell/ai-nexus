    "typescript": "^5.9.3",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cross-env": "^10.1.0",
