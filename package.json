{
  "name": "ai-nexus",
  "version": "2.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "clean": "node -e \"const fs=require('fs');const p='.astro';fs.existsSync(p)&&fs.rmSync(p,{recursive:true})\"",
    "build": "npm run clean && cross-env NODE_ENV=production astro build",
    "preview": "astro preview",
    "fetch-data": "node scripts/fetch-data.js",
    "fetch-data:v3": "node scripts/fetch-data-v3.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "ces-check": "node scripts/ces-check.cjs",
    "ces": "npm run ces-check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/cloudflare": "^12.6.11",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^6.0.2",
    "@aws-sdk/client-s3": "^3.937.0",
    "@cloudflare/ai": "^1.2.2",
    "@cloudflare/workers-types": "^4.20251121.0",
    "@google/generative-ai": "^0.18.0",
    "@iconify-json/mdi": "^1.1.67",
    "@tailwindcss/typography": "^0.5.19",
    "astro": "^5.16.0",
    "base64-js": "^1.5.1",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.5",
    "fuse.js": "^7.1.0",
    "marked": "^13.0.3",
    "mustache": "^4.2.0",
    "react": "^19.0.0-rc-f994737d14-20240522",
    "react-dom": "^19.0.0-rc-f994737d14-20240522",
    "sharp": "^0.34.5",
